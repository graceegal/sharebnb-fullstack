{
    "$schema": "http://json-schema.org/draft-07/schema#",
    "$id": "http://our.company.com/companySearch.schema.json",
    "type": "object",
    "properties": {
      "title": {
        "type": "string",
        "minLength": 1,
        "maxLength": 250
      },
      "address": {
        "type": "string",
        "minLength": 1
      },
      "description": {
        "type": "string",
        "minLength": 1
      },
      "price": {
        "type": "integer",
        "minimum": 1
      }
    },
    "additionalProperties": false,
    "required": [
        "title",
        "address",
        "description",
        "price"
    ]
  }
